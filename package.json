{
  "name": "yasminchat",
  "version": "0.1.0",
  "description": "A simple socket.io chat",
  "main": "index.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/socket.io": "^2.1.8",
    "express": "^4.15.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "build": "rimraf ./dist && run-p build:* ",
    "build:server": "tsc src/server/*.ts --outDir dist/server",
    "build:webapp": "webpack --config webpack.config.js --mode=production",
    "start:prod": "node ./dist/server && http-server dist/webapp/",
    "start": "run-p develop:*",
    "develop:server": "nodemon",
    "develop:webapp": "webpack-dev-server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roberto14/yasminchat.git"
  },
